--ESP or idkkk726
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Extended fake executor list
local executors = {
    "Synapse X",
    "KRNL",
    "Fluxus",
    "Delta",
    "Oxygen U",
    "Script-Ware",
    "Evon",
    "Arceus X",
    "Nihon",
    "Vega X",
    "Codex",
    "Valyse",
    "Sentinel",
    "Coco Z",
    "Hydrogen",
    "Carnal",
    "JJSploit",
    "Frost X",
    "RC7",
    "SirHurt",
    "Celery",
    "None cause they're innocent"
}

-- Assign a random executor
local function getFakeExecutor()
    return executors[math.random(1, #executors)]
end

-- Create ESP for player
local function createESP(player)
    if player == LocalPlayer then return end -- skip yourself

    player.CharacterAdded:Connect(function(character)
        repeat task.wait() until character:FindFirstChild("HumanoidRootPart")

        local billboard = Instance.new("BillboardGui")
        billboard.Adornee = character:WaitForChild("HumanoidRootPart")
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.TextColor3 = Color3.fromRGB(0, 255, 0)
        label.Font = Enum.Font.SourceSansBold
        label.TextScaled = true
        label.TextStrokeTransparency = 0.5
        label.Parent = billboard

        -- Fake executor assignment
        local fakeExec = getFakeExecutor()
        label.Text = player.Name .. "\nExecutor: " .. fakeExec

        billboard.Parent = character
    end)
end

-- Add ESP to all current players
for _, player in ipairs(Players:GetPlayers()) do
    createESP(player)
end

-- Add ESP to future players
Players.PlayerAdded:Connect(createESP)
